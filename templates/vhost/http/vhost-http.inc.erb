# generated by puppet from apache/templates/http/vhost-http.inc.erb
# used by apache/manifests/vhost/http.pp
#
<VirtualHost *:80>

    ServerName <%= @server_name %>
<% if @server_aliases -%>
<% @server_aliases.each do |server_alias| -%>
    ServerAlias <%= server_alias %>
<% end -%>
<% end -%>

    CustomLog logs/<%= @name %>_access_log combined
    CustomLog logs/<%= @name %>-access_timed_log timed_combined
    ErrorLog logs/<%= @name %>_error_log

    Include conf.d/container-<%= @name %>.inc

</VirtualHost>
